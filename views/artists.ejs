            <div class="row align-items-center">
                <section>
                    <div class="parallax"></div>
                    <section class="container-fluid px-0">
                        <!-- artists 1 -->
                        <% artists.forEach(artistObj=> { %>


                            <div class="row align-items-center content">
                                <div class="col-md-6 order-2 order-md-1">
                                    <img class="artistImg" src="/images/artists/<%= artistObj.stageName %>_tn.jpg"
                                        alt="">
                                </div>
                                <div class="col-md-6 text-center order-1 order-md-2">
                                    <div class="row justify-content-center">
                                        <div class="col-10 col-lg-8 blurb mb-5 mb-md-0">
                                            <h2>
                                                <%= artistObj.albumName %>
                                            </h2>
                                            <p><a href=""></a></p>
                                            <p><a href="/artists/<%= artistObj.stageName%>">by <%= artistObj.stageName
                                                        %></a> </p>
                                            <p class="summary">
                                                <%= artistObj.summary %>
                                            </p>
                                            <ul>
                                                <% tracks.forEach(picture=> { %>
                                                    Top tracks: <li class="topTracks">
                                                        <%= picture %>
                                                    </li>
                                                    <% }) %>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <% }) %>

